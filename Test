<html>
<head>
</head>
<body>
  <label for="stg">Stg:</label>
  <input type="text" id="stg" name="stg"><br>

  <label for="spd">Spd:</label>
  <input type="text" id="spd" name="spd"><br>

  <label for="shot">Shot:</label>
  <input type="text" id="shot" name="shot"><br>

  <label for="pass">Pass:</label>
  <input type="text" id="pass" name="pass"><br>

  <label for="tech">Tech:</label>
  <input type="text" id="tech" name="tech"><br>

  <button type="button" onclick="filter()">Filter</button>

  <ul id="results"></ul>
</body>

<script src="builds3.js"></script>
<script>
function filter() {
  const stgValue = document.getElementById("stg").value;
  const spdValue = document.getElementById("spd").value;
  const shotValue = document.getElementById("shot").value;
  const passValue = document.getElementById("pass").value;
  const techValue = document.getElementById("tech").value;

  const filteredPlayers = players.filter(player =>
    player.stg >= stgValue &&
    player.spd >= spdValue &&
    player.shot >= shotValue &&
    player.pass >= passValue &&
    player.tech >= techValue
  );

  const resultsList = document.getElementById("results");
  resultsList.innerHTML = "";

  filteredPlayers.forEach(player => {
    const listItem = document.createElement("li");
    listItem.innerText = player.name;
    resultsList.appendChild(listItem);
  });
}
</script>
</html>
