<input type="number" id="spd-min">

<label for="shot-min">Minimum Shot:</label>
<input type="number" id="shot-min">

<label for="pass-min">Minimum Pass:</label>
<input type="number" id="pass-min">

<label for="tech-min">Minimum Technique:</label>
<input type="number" id="tech-min">

<button onclick="findMatchingBuilds()">Find Matching Builds</button>

</head>

<body>



<script>

function findMatchingBuilds() {
  // Retrieve the minimum values from the input boxes
  const strMin = parseInt(document.getElementById('str-min').value);
  const spdMin = parseInt(document.getElementById('spd-min').value);
  const shotMin = parseInt(document.getElementById('shot-min').value);
  const passMin = parseInt(document.getElementById('pass-min').value);
  const techMin = parseInt(document.getElementById('tech-min').value);

  // Filter the Builds array to find the matching builds
  const matchingBuilds = Builds.filter(build => (
    build.Str >= strMin &&
    build.Spd >= spdMin &&
    build.Shot >= shotMin &&
    build.Pass >= passMin &&
    build.Tech >= techMin
  ));

  // Display the matching builds on the webpage
  const resultsContainer = document.getElementById('results');
  resultsContainer.innerHTML = '';

  if (matchingBuilds.length > 0) {
    const header = document.createElement('h2');
    header.textContent = 'Matching Builds:';
    resultsContainer.appendChild(header);

    const list = document.createElement('ul');
    matchingBuilds.forEach(build => {
      const item = document.createElement('li');
      item.textContent = `Gear: ${build.Gear}, Str: ${build.Str}, Spd: ${build.Spd}, Shot: ${build.Shot}, Pass: ${build.Pass}, Tech: ${build.Tech}`;
      list.appendChild(item);
    });
    resultsContainer.appendChild(list);
  } else {
    const message = document.createElement('p');
    message.textContent = 'No matching builds found.';
    resultsContainer.appendChild(message);
  }
}

</script>

</head>
